<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.gdut.mapper.TbOrderMapper">
    <insert id="addOrder" parameterType="net.gdut.bean.TbOrder">
        insert into tb_order (uno,bno,quantity) values (#{uno},#{bno},#{quantity})
    </insert>
</mapper>